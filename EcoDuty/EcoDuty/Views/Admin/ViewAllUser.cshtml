@model IEnumerable<User>
@{
    ViewData["Title"] = "ViewAllUser";
}

<h1>ViewAllUser</h1>
<br />
@foreach (var user in Model)
{
    <div>
        <a href='@Url.Action("UserPage","Admin", new{id = user.Id})' class="link__dish">@user.Passport @user.Surname @user.Name @user.Patronymic @user.Role</a>
        @*<a href='@Url.Action("PlaceTypeRemove","Admin",new{id = placeType.Id})' class="link__dish">Delete</a>*@
    </div>
}

<a href='@Url.Action("Index","Admin")' class="back__list">Back</a>
